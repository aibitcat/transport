{
  "runtime": {
    "DEBUG": true,
    "HTTP":"0.0.0.0:12347",
    "MAXPROCS": 4,
    "VERSION": "0.0.2"
  },
  "inputs": {
    "files": {
      "path": [
        "test/logs/kv.log"
      ],
      "endstop":false
    }
  },
  "handles": {
    "kv": {
        "keys": ["time","Level","msg"],
        "split": "||"
    }
  },
  "outputs": {
    "file": {
      "path": "/tmp/test.test"
    },
    "std":{}
  }
}
